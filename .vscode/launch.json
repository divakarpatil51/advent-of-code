{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "cwd": "${fileDirname}",
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "python",
                "PYDEVD_CONTAINER_RANDOM_ACCESS_MAX_ITEMS": "10000"
            }
        }
    ]
}

